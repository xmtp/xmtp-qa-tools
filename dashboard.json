    {
      "id": 202034964114978,
      "definition": {
        "title": "Daily Error Count vs Error Lines",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value", "sum"],
        "type": "timeseries",
        "requests": [
          {
            "queries": [
              {
                "name": "query1",
                "data_source": "logs",
                "search": {
                  "query": "@service:xmtp-qa-tools @env:production @region:us-east"
                },
                "indexes": ["*"],
                "group_by": [],
                "compute": {
                  "aggregation": "count",
                  "rollup": { "type": "day" }
                },
                "storage": "hot"
              },
              {
                "name": "query2",
                "data_source": "logs",
                "search": {
                  "query": "@service:xmtp-qa-tools @env:production @region:us-east "
                },
                "indexes": ["*"],
                "group_by": [],
                "compute": {
                  "aggregation": "sum",
                  "metric": "@failLines",
                  "rollup": { "type": "day" }
                },
                "storage": "hot"
              }
            ],
            "formulas": [
              { "alias": "Daily Error Occurrences", "formula": "query1" },
              { "alias": "Daily Error Lines Total", "formula": "query2" }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "dog_classic",
              "order_by": "values",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "line"
          }
        ]
      },
      "layout": { "x": 0, "y": 10, "width": 4, "height": 2 }
    },
    {
      "id": 403905557672626,
      "definition": {
        "title": "Performance by region",
        "title_size": "16",
        "title_align": "left",
        "type": "geomap",
        "requests": [
          {
            "queries": [
              {
                "name": "query1",
                "data_source": "metrics",
                "query": "avg:xmtp.sdk.duration{metric_type:network,network_phase:server_call} by {country_iso_code}"
              }
            ],
            "response_format": "scalar",
            "formulas": [
              { "alias": "Server call duration (ms)", "formula": "query1" }
            ]
          }
        ],
        "style": { "palette": "green_to_orange", "palette_flip": false },
        "view": { "focus": "WORLD" }
      },
      "layout": { "x": 4, "y": 10, "width": 3, "height": 5 }
    },
    {
      "id": 5297767210484191,
      "definition": {
        "title": "Fail lines",
        "title_size": "16",
        "title_align": "left",
        "requests": [
          {
            "response_format": "event_list",
            "query": {
              "data_source": "logs_stream",
              "query_string": "@service:xmtp-qa-tools",
              "indexes": [],
              "storage": "hot"
            },
            "columns": [
              { "field": "status_line", "width": "auto" },
              { "field": "timestamp", "width": "auto" },
              { "field": "env", "width": "auto" },
              { "field": "region", "width": "auto" },
              { "field": "failLines", "width": "auto" },
              { "field": "test", "width": "auto" }
            ]
          }
        ],
        "type": "list_stream"
      },
      "layout": { "x": 0, "y": 12, "width": 4, "height": 4 }
    }
  ],
  "template_variables": [
    {
      "name": "env",
      "prefix": "env",
      "available_values": ["dev", "production", "local"],
      "default": "production"
    },
    {
      "name": "region",
      "prefix": "region",
      "available_values": [
        "south-america",
        "europe",
        "asia",
        "us-east",
        "us-west"
      ],
      "default": "us-east"
    },
    {
      "name": "test",
