              }
            ],
            "response_format": "scalar",
            "sort": {
              "order_by": [
                { "type": "group", "name": "region", "order": "asc" }
              ],
              "count": 500
            },
            "formulas": [
              { "alias": "Production (ms)", "formula": "query1" },
              { "alias": "Dev (ms)", "formula": "query2" }
            ]
          }
        ],
        "has_search_bar": "auto"
