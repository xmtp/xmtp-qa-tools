{
  "title": "Protocol Performance Stats",
  "description": "Group performance stats for qa-tools. Includes latency percentiles, sample counts, throughput, and failure rates.",
  "widgets": [
    {
      "id": 501001,
      "definition": {
        "type": "note",
        "content": "This dashboard tracks group stats metrics (`metric_family:group_stats_v1`) emitted by `monitoring/group-stats.test.ts`. Percentile policy: track avg, p50, and p95. Treat p95 as valid once `sample_count >= 100`.",
        "background_color": "white",
        "font_size": "14",
        "text_align": "left",
        "vertical_align": "center",
        "show_tick": false,
        "tick_pos": "50%",
        "tick_edge": "left",
        "has_padding": true
      },
      "layout": {
        "x": 0,
        "y": 0,
        "width": 12,
        "height": 1
      }
    },
    {
      "id": 501010,
      "definition": {
        "title": "p95 Group Latency (ms)",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "response_format": "scalar",
            "queries": [
              {
                "name": "query1",
                "data_source": "metrics",
                "query": "avg:xmtp.sdk.duration.calc.p95{metric_family:group_stats_v1,metric_type:operation,$env,$region,$sdk}",
                "aggregator": "avg"
              }
            ],
            "formulas": [
              {
                "formula": "query1"
              }
            ],
            "conditional_formats": [
              {
                "comparator": "<=",
                "value": 1000,
                "palette": "white_on_green"
              },
              {
                "comparator": "<=",
                "value": 3000,
                "palette": "white_on_yellow"
              },
              {
                "comparator": ">",
                "value": 3000,
                "palette": "white_on_red"
              }
            ]
          }
        ],
        "autoscale": true,
        "precision": 0
      },
      "layout": {
        "x": 0,
        "y": 1,
        "width": 6,
        "height": 2
      }
    },
    {
      "id": 501011,
      "definition": {
        "title": "Operation Success Rate (%)",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "response_format": "scalar",
            "queries": [
              {
                "name": "ok_ops",
                "data_source": "metrics",
                "query": "sum:xmtp.sdk.operation_count{metric_family:group_stats_v1,status:success,$env,$region,$sdk}",
                "aggregator": "sum"
              },
              {
                "name": "err_ops",
                "data_source": "metrics",
                "query": "sum:xmtp.sdk.operation_count{metric_family:group_stats_v1,status:error,$env,$region,$sdk}",
                "aggregator": "sum"
              }
            ],
            "formulas": [
              {
                "formula": "(100 * ok_ops) / (ok_ops + default_zero(err_ops) + 0.000001)"
              }
            ],
            "conditional_formats": [
              {
                "comparator": ">=",
                "value": 99,
                "palette": "white_on_green"
              },
              {
                "comparator": ">=",
                "value": 95,
                "palette": "white_on_yellow"
              },
              {
                "comparator": "<",
                "value": 95,
                "palette": "white_on_red"
              }
            ]
          }
        ],
        "autoscale": true,
        "precision": 1
      },
      "layout": {
        "x": 6,
        "y": 1,
        "width": 6,
        "height": 2
      }
    },
    {
      "id": 501015,
      "definition": {
        "title": "Dev vs testnet-staging stats by operation",
        "title_size": "16",
        "title_align": "left",
        "type": "query_table",
        "requests": [
          {
            "queries": [
              {
                "data_source": "metrics",
                "name": "avg_dev",
                "query": "avg:xmtp.sdk.duration{metric_family:group_stats_v1,metric_type:operation,env:dev,$region,$sdk,$members_bucket} by {operation}",
                "aggregator": "avg"
              },
              {
                "data_source": "metrics",
                "name": "avg_tns",
                "query": "avg:xmtp.sdk.duration{metric_family:group_stats_v1,metric_type:operation,env:testnet-staging,$region,$sdk,$members_bucket} by {operation}",
                "aggregator": "avg"
              },
              {
                "data_source": "metrics",
                "name": "p95_dev",
                "query": "avg:xmtp.sdk.duration.calc.p95{metric_family:group_stats_v1,metric_type:operation,env:dev,$region,$sdk,$members_bucket} by {operation}",
                "aggregator": "avg"
              },
              {
                "data_source": "metrics",
                "name": "p95_tns",
                "query": "avg:xmtp.sdk.duration.calc.p95{metric_family:group_stats_v1,metric_type:operation,env:testnet-staging,$region,$sdk,$members_bucket} by {operation}",
                "aggregator": "avg"
              },
              {
                "data_source": "metrics",
                "name": "ok_dev",
                "query": "sum:xmtp.sdk.operation_count{metric_family:group_stats_v1,status:success,env:dev,$region,$sdk,$members_bucket} by {operation}",
                "aggregator": "sum"
              },
              {
                "data_source": "metrics",
                "name": "err_dev",
                "query": "sum:xmtp.sdk.operation_count{metric_family:group_stats_v1,status:error,env:dev,$region,$sdk,$members_bucket} by {operation}",
                "aggregator": "sum"
              },
              {
                "data_source": "metrics",
                "name": "ok_tns",
                "query": "sum:xmtp.sdk.operation_count{metric_family:group_stats_v1,status:success,env:testnet-staging,$region,$sdk,$members_bucket} by {operation}",
                "aggregator": "sum"
              },
              {
                "data_source": "metrics",
                "name": "err_tns",
                "query": "sum:xmtp.sdk.operation_count{metric_family:group_stats_v1,status:error,env:testnet-staging,$region,$sdk,$members_bucket} by {operation}",
                "aggregator": "sum"
              }
            ],
            "response_format": "scalar",
            "sort": {
              "count": 500,
              "order_by": [
                {
                  "type": "group",
                  "name": "operation",
                  "order": "asc"
                }
              ]
            },
            "formulas": [
              {
                "cell_display_mode": "number",
                "alias": "avg_dev_ms",
                "formula": "avg_dev"
              },
              {
                "cell_display_mode": "number",
                "alias": "avg_testnet_ms",
                "formula": "avg_tns"
              },
              {
                "cell_display_mode": "number",
                "alias": "avg_diff_ms",
                "formula": "avg_tns - avg_dev"
              },
              {
                "cell_display_mode": "number",
                "alias": "p95_dev_ms",
                "formula": "p95_dev"
              },
              {
                "cell_display_mode": "number",
                "alias": "p95_testnet_ms",
                "formula": "p95_tns"
              },
              {
                "cell_display_mode": "number",
                "alias": "p95_diff_ms",
                "formula": "p95_tns - p95_dev"
              },
              {
                "cell_display_mode": "number",
                "alias": "failure_dev_pct",
                "formula": "(100 * default_zero(err_dev)) / (ok_dev + default_zero(err_dev) + 0.000001)"
              },
              {
                "cell_display_mode": "number",
                "alias": "failure_testnet_pct",
                "formula": "(100 * default_zero(err_tns)) / (ok_tns + default_zero(err_tns) + 0.000001)"
              },
              {
                "cell_display_mode": "number",
                "alias": "failure_diff_pct",
                "formula": "((100 * default_zero(err_tns)) / (ok_tns + default_zero(err_tns) + 0.000001)) - ((100 * default_zero(err_dev)) / (ok_dev + default_zero(err_dev) + 0.000001))"
              }
            ]
          }
        ],
        "has_search_bar": "auto"
      },
      "layout": {
        "x": 0,
        "y": 3,
        "width": 12,
        "height": 8
      }
    }
  ],
  "template_variables": [
    {
      "name": "env",
      "prefix": "env",
      "available_values": [
        "dev",
        "production",
        "testnet-staging"
      ],
      "default": "production"
    },
    {
      "name": "region",
      "prefix": "region",
      "available_values": [],
      "default": "*"
    },
    {
      "name": "sdk",
      "prefix": "sdk",
      "available_values": [],
      "default": "*"
    },
    {
      "name": "members_bucket",
      "prefix": "members_bucket",
      "available_values": [
        "small",
        "medium",
        "large"
      ],
      "default": "*"
    }
  ],
  "layout_type": "ordered",
  "notify_list": [],
  "pause_auto_refresh": false,
  "reflow_type": "fixed"
}
