{
  "name": "@xmtp/load-test",
  "version": "1.0.0",
  "description": "High-performance load testing framework for XMTP",
  "type": "module",
  "scripts": {
    "setup": "tsx setup.ts",
    "build": "node build-processor.js",
    "test": "tsx adaptive-test.ts",
    "test:fixed": "yarn build && artillery run artillery-config.yml",
    "test:burst": "yarn build && artillery run artillery-config-burst.yml",
    "test:auto": "yarn build && artillery run artillery-config-auto.yml",
    "test:simple": "tsx run-simple.ts",
    "auto-tune": "tsx auto-tune.ts",
    "analyze": "tsx analyze-results.ts",
    "clean": "rm -rf data/ *.db3 report.json artillery-processor.js artillery-processor.cjs dist/"
  },
  "keywords": [
    "xmtp",
    "load-test",
    "performance"
  ],
  "dependencies": {
    "@xmtp/node-sdk": "^4.5.0",
    "artillery": "^2.0.20",
    "commander": "^12.0.0",
    "viem": "^2.21.54",
    "uint8arrays": "^5.0.1"
  },
  "devDependencies": {
    "tsx": "^4.7.1",
    "@types/node": "^20.11.19",
    "esbuild": "^0.20.0",
    "typescript": "^5.3.3"
  }
}

