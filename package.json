{
  "name": "xmtp-qa-tools",
  "version": "0.4.16",
  "private": true,
  "type": "module",
  "workspaces": [
    "agents/bots/*",
    "helpers",
    "workers"
  ],
  "scripts": {
    "agent-versions": "tsx cli/versions.ts",
    "bot": "tsx cli/bot.ts",
    "build": "tsc",
    "c": "yarn build && yarn lint && yarn format",
    "clean": "rimraf .data/ ||: && rimraf logs/ ||  :",
    "fork": "tsx forks/cli.ts",
    "format": "prettier -w .",
    "gen": "tsx cli/gen.ts",
    "gen:keys": "tsx scripts/generateKeys.ts",
    "inboxes:restart": "yarn cli/gen.ts --installations 2 --count 200 --restart",
    "inboxes:update": "yarn cli/gen.ts --installations 2",
    "lint": "eslint .",
    "monitor:yarn": "httpstat https://grpc.dev.xmtp.network:443",
    "record": "npx playwright codegen 'https://xmtp.chat/'",
    "regression": "yarn versions && yarn test performance --env dev --sync all --versions 3 --size 10-100 && yarn test bugs",
    "revoke": "tsx scripts/revokeInstallations.ts",
    "send": "tsx cli/send.ts",
    "test": "tsx cli/test.ts",
    "versions": "tsx cli/versions.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@xmtp/agent-sdk-1.1.10": "npm:@xmtp/agent-sdk@1.1.10",
    "@xmtp/agent-sdk-1.1.2": "npm:@xmtp/agent-sdk@1.1.2",
    "@xmtp/agent-sdk-1.1.5": "npm:@xmtp/agent-sdk@1.1.5",
    "@xmtp/content-type-markdown": "^1.0.0",
    "@xmtp/content-type-reaction": "^2.0.2",
    "@xmtp/content-type-reply": "^2.0.2",
    "@xmtp/content-type-wallet-send-calls": "^2.0.0",
    "@xmtp/node-bindings-1.3.3": "npm:@xmtp/node-bindings@1.3.3",
    "@xmtp/node-bindings-1.3.4": "npm:@xmtp/node-bindings@1.3.4",
    "@xmtp/node-bindings-1.3.5": "npm:@xmtp/node-bindings@1.3.5",
    "@xmtp/node-bindings-1.3.6": "npm:@xmtp/node-bindings@1.3.6",
    "@xmtp/node-bindings-1.4.0": "npm:@xmtp/node-bindings@1.4.0",
    "@xmtp/node-bindings-1.5.4": "npm:@xmtp/node-bindings@1.5.4",
    "@xmtp/node-bindings-1.6.1": "npm:@xmtp/node-bindings@1.6.1-rc3",
    "@xmtp/node-sdk-3.2.2": "npm:@xmtp/node-sdk@3.2.2",
    "@xmtp/node-sdk-4.0.1": "npm:@xmtp/node-sdk@4.0.1",
    "@xmtp/node-sdk-4.0.2": "npm:@xmtp/node-sdk@4.0.2",
    "@xmtp/node-sdk-4.0.3": "npm:@xmtp/node-sdk@4.0.3",
    "@xmtp/node-sdk-4.1.0": "npm:@xmtp/node-sdk@4.1.0",
    "@xmtp/node-sdk-4.2.3": "npm:@xmtp/node-sdk@4.2.3",
    "@xmtp/node-sdk-4.3.0": "npm:@xmtp/node-sdk@4.3.0",
    "axios": "^1.8.2",
    "datadog-metrics": "^0.12.1",
    "dotenv": "^16.5.0",
    "node-fetch": "^3.3.0",
    "rimraf": "^6.0.1",
    "uint8arrays": "^5.1.0",
    "viem": "^2",
    "vitest": "^3.2.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "latest",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.0.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "playwright-chromium": "latest",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.22.0"
  },
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">=20"
  }
}
