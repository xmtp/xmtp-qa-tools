{
  "name": "xmtp-qa-tools",
  "version": "0.2.30",
  "private": true,
  "type": "module",
  "workspaces": [
    "bots/*",
    "helpers",
    "workers"
  ],
  "scripts": {
    "ai": "tsx cli/ai.ts",
    "ansi:clean": "tsx -e \"(async () => { const { cleanAllRawLogs } = await import('./helpers/analyzer.js'); await cleanAllRawLogs(); })();\"",
    "ansi:forks": "tsx -e \"(async () => { const { cleanForksLogs } = await import('./helpers/analyzer.js'); await cleanForksLogs(); })();\"",
    "bot": "tsx cli/bot.ts",
    "build": "tsc",
    "clean": "rimraf .data/ ||: && rimraf logs/ ||  :",
    "format": "prettier -w .",
    "gen": "tsx cli/gen.ts",
    "groups": "tsx cli/groups.ts",
    "lint": "eslint .",
    "permissions": "tsx cli/permissions.ts",
    "monitor:yarn": "httpstat https://grpc.dev.xmtp.network:443",
    "record": "npx playwright codegen 'https://xmtp.chat/'",
    "regression": "yarn test functional --env local --sync all --versions 3 --size 5-10-50-100",
    "revoke": "tsx cli/revoke.ts",
    "send": "tsx cli/send.ts",
    "test": "tsx cli/test.ts",
    "ui": "vitest --ui --standalone --watch",
    "update:local": "yarn gen --envs local --installations 2 --count 500",
    "update:prod": "yarn gen --envs production --installations 2",
    "versions": "tsx cli/versions.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@xmtp/content-type-reaction": "^2.0.2",
    "@xmtp/content-type-reply": "^2.0.2",
    "@xmtp/node-bindings": "npm:@xmtp/node-bindings@1.3.3",
    "@xmtp/node-bindings-0.0.9": "npm:@xmtp/mls-client-bindings-node@0.0.9",
    "@xmtp/node-bindings-0.4.1": "npm:@xmtp/node-bindings@0.0.41",
    "@xmtp/node-bindings-1.0.0": "npm:@xmtp/node-bindings@1.0.0",
    "@xmtp/node-bindings-1.1.3": "npm:@xmtp/node-bindings@1.1.3",
    "@xmtp/node-bindings-1.1.8": "npm:@xmtp/node-bindings@1.1.8",
    "@xmtp/node-bindings-1.2.0": "npm:@xmtp/node-bindings@1.2.0",
    "@xmtp/node-bindings-1.2.2": "npm:@xmtp/node-bindings@1.2.2",
    "@xmtp/node-bindings-1.2.5": "npm:@xmtp/node-bindings@1.2.5",
    "@xmtp/node-bindings-1.2.7": "npm:@xmtp/node-bindings@1.2.7",
    "@xmtp/node-bindings-1.2.8": "npm:@xmtp/node-bindings@1.2.8",
    "@xmtp/node-bindings-1.3.0": "npm:@xmtp/node-bindings@1.3.0",
    "@xmtp/node-bindings-1.3.1": "npm:@xmtp/node-bindings@1.3.1",
    "@xmtp/node-bindings-1.3.2": "npm:@xmtp/node-bindings@1.3.2",
    "@xmtp/node-bindings-1.3.3": "npm:@xmtp/node-bindings@1.3.3",
    "@xmtp/node-sdk": "npm:@xmtp/node-sdk@4.0.0",
    "@xmtp/node-sdk-0.0.13": "npm:@xmtp/mls-client@0.0.13",
    "@xmtp/node-sdk-0.0.47": "npm:@xmtp/node-sdk@0.0.47",
    "@xmtp/node-sdk-1.0.5": "npm:@xmtp/node-sdk@1.0.5",
    "@xmtp/node-sdk-2.0.9": "npm:@xmtp/node-sdk@2.0.9",
    "@xmtp/node-sdk-2.1.0": "npm:@xmtp/node-sdk@2.1.0",
    "@xmtp/node-sdk-2.2.1": "npm:@xmtp/node-sdk@2.2.1",
    "@xmtp/node-sdk-3.0.1": "npm:@xmtp/node-sdk@3.0.1",
