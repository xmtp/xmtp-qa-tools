{
  "name": "xmtp-qa-tools",
  "version": "0.1.10",
  "private": true,
  "type": "module",
  "workspaces": [
    "bots/*",
    "helpers",
    "workers"
  ],
  "scripts": {
    "bot": "yarn cli bot",
    "build": "tsc",
    "clean": "rimraf .data/ ||: && rimraf logs/ ||  :",
    "cli": "tsx scripts/cli.ts",
    "format": "prettier -w .",
    "functional": "npx vitest run functional --pool=forks --fileParallelism=false",
    "gen": "tsx scripts/gen.ts",
    "gen:keys": "tsx scripts/gen.ts",
    "large": "npx vitest run suites/metrics/large --pool=forks --fileParallelism=false",
    "lint": "eslint .",
    "monitor:dev": "httpstat https://grpc.dev.xmtp.network:443",
    "railway": "railway run yarn test ",
    "record": "npx playwright codegen 'https://xmtp.chat/'",
    "script": "yarn cli script",
    "start": "vitest --ui --standalone --watch ",
    "test": "yarn cli test"
  },
