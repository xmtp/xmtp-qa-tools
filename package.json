{
  "name": "xmtp-qa-tools",
  "version": "0.2.12",
  "private": true,
  "type": "module",
  "workspaces": [
    "qa-agent",
    "bots/*",
    "helpers",
    "workers"
  ],
  "scripts": {
    "ansi:clean": "tsx -e \"(async () => { const { cleanAllRawLogs } = await import('./helpers/analyzer.js'); await cleanAllRawLogs(); })();\"",
    "ansi:forks": "tsx -e \"(async () => { const { cleanAllRawLogs } = await import('./helpers/analyzer.js'); await cleanAllRawLogs('Fork detected'); })();\"",
    "bench": "yarn test suites/bench/bench.test.ts",
    "bot": "yarn cli bot",
    "build": "tsc",
    "claude": "npx @anthropic-ai/claude-code",
    "clean": "rimraf .data/ ||: && rimraf logs/ ||  :",
    "cli": "tsx scripts/cli.ts",
    "dms": "yarn test functional/dms.test.ts",
    "format": "prettier -w .",
    "functional": "yarn test suites/functional --sync all",
    "gen": "tsx inboxes/gen.ts",
    "large": "yarn test suites/metrics/large",
    "lint": "eslint .",
    "local-update": "./inboxes/gen.sh --envs local --installations 2,5 --count 500",
    "monitor:yarn": "httpstat https://grpc.dev.xmtp.network:443",
    "prod-update": "./inboxes/gen.sh --envs production --installations 2",
    "record": "npx playwright codegen 'https://xmtp.chat/'",
    "regression": "yarn test suites/functional --versions 3",
    "run:agent": "./suites/agents/run.sh",
    "run:commits": "./suites/commits/run.sh",
    "script": "yarn cli script",
    "test": "yarn cli test",
    "ui": "vitest --ui --standalone --watch",
    "workflow-failure": "tsx -e \"import { workflowFailed } from './helpers/analyzer'; (async () => { await workflowFailed(process.argv[1] || 'Unknown'); })();\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@xmtp/content-type-reaction": "^2.0.2",
    "@xmtp/content-type-reply": "^2.0.2",
    "@xmtp/node-bindings": "1.2.8",
    "@xmtp/node-bindings-0.0.9": "npm:@xmtp/mls-client-bindings-node@0.0.9",
    "@xmtp/node-bindings-0.4.1": "npm:@xmtp/node-bindings@0.0.41",
    "@xmtp/node-bindings-1.0.0": "npm:@xmtp/node-bindings@1.0.0",
    "@xmtp/node-bindings-1.1.3": "npm:@xmtp/node-bindings@1.1.3",
    "@xmtp/node-bindings-1.1.8": "npm:@xmtp/node-bindings@1.1.8",
    "@xmtp/node-bindings-1.2.0": "npm:@xmtp/node-bindings@1.2.0",
    "@xmtp/node-bindings-1.2.2": "npm:@xmtp/node-bindings@1.2.2",
    "@xmtp/node-bindings-1.2.5": "npm:@xmtp/node-bindings@1.2.5",
    "@xmtp/node-bindings-1.2.7": "npm:@xmtp/node-bindings@1.2.7",
    "@xmtp/node-bindings-1.2.8": "npm:@xmtp/node-bindings@1.2.8",
    "@xmtp/node-sdk": "3.1.2",
    "@xmtp/node-sdk-0.0.13": "npm:@xmtp/mls-client@0.0.13",
    "@xmtp/node-sdk-0.0.47": "npm:@xmtp/node-sdk@0.0.47",
    "@xmtp/node-sdk-1.0.5": "npm:@xmtp/node-sdk@1.0.5",
    "@xmtp/node-sdk-2.0.9": "npm:@xmtp/node-sdk@2.0.9",
    "@xmtp/node-sdk-2.1.0": "npm:@xmtp/node-sdk@2.1.0",
    "@xmtp/node-sdk-2.2.1": "npm:@xmtp/node-sdk@2.2.1",
    "@xmtp/node-sdk-3.0.1": "npm:@xmtp/node-sdk@3.0.1",
    "@xmtp/node-sdk-3.1.1": "npm:@xmtp/node-sdk@3.1.1",
    "@xmtp/node-sdk-3.1.2": "npm:@xmtp/node-sdk@3.1.2",
    "axios": "^1.8.2",
    "datadog-metrics": "^0.12.1",
    "dotenv": "^16.5.0",
    "node-fetch": "^3.3.0",
    "rimraf": "^6.0.1",
    "uint8arrays": "^5.1.0",
    "viem": "^2",
    "vitest": "^3.2.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.24",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@playwright/test": "^1.50.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.0.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "playwright-chromium": "^1.50.1",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.22.0"
  },
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">=20"
  },
